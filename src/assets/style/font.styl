@import "../../theme.styl"
@import '~cube-ui/src/common/stylus/variable.styl'

@font-face {
  font-family: 'iconfont';  /* project id 867509 */
  src: url('//at.alicdn.com/t/font_867509_5ip6o3yvz5q.eot');
  src: url('//at.alicdn.com/t/font_867509_5ip6o3yvz5q.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_867509_5ip6o3yvz5q.woff') format('woff'),
  url('//at.alicdn.com/t/font_867509_5ip6o3yvz5q.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_867509_5ip6o3yvz5q.svg#iconfont') format('svg');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

